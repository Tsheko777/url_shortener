spring.application.name=url_shortener
spring.datasource.url=jdbc:mysql://mysql:3306/yourdb
spring.datasource.username=username
spring.datasource.password=password
spring.jpa.show-sql=true
spring.jpa.hibernate.ddl-auto=update
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQLDialect
spring.jpa.properties.hibernate.format_sql=true

spring.cache.type=redis
spring.cache.redis.time-to-live=10m
spring.data.redis.host=redis
spring.data.redis.port=6379

app.countryApi=https://api.country.is/

logging.level.org.springframework.cache=DEBUG
logging.level.org.springframework.data.redis.cache=DEBUG
logging.level.org.springframework.data.redis.core=DEBUG
logging.level.org.springframework.cache=DEBUG
logging.level.org.springframework.cache.interceptor.CacheInterceptor=TRACE
logging.level.org.springframework.data.redis=DEBUG

